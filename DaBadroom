local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Da Badrooms", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--TABS--
local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local SwordTab = Window:MakeTab({
	Name = "Collect Swords",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local LevelsTab = Window:MakeTab({
	Name = "Exit Level",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MiscTab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

